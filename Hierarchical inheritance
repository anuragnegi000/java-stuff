//Hierarchical inheritance.
class Animal{
    public void eat(){
        System.out.println("Eating....");
    }
}
class Dog extends Animal{
    public void bark(){
        System.out.println("Barking....");
    }
}
class Cat extends Animal{
    public void meow(){
        System.out.println("Meowing....");
    }
}
public class java_cetpa11 {
    public static void main(String[] args) {
        Cat obj=new Cat();
        obj.meow();
        obj.eat();
//        obj.bark    (This function will not work because we have use extend as animal in class Cat)
    }
}
